<%= month_calendar(attribute: :created_at, events: @diaries) do |date, diaries| %>

  <%= date.day %>

  <% diaries.each do |diary| %>
    <div class="calender_event">
      <%= link_to diary_path(diary) do %>
        <h3 class="bi bi-flower3 text-danger ms-2"></h3>
      <% end %>
    </div>
  <% end %>
<% end %>